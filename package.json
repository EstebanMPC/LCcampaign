{
  "name": "luisdiego",
  "version": "1.0.0",
  "description": "campaign website",
  "main": "index.js",
  "dependencies": {
    "animate.css": "^4.1.1",
    "bootstrap": "^5.1.3",
    "lite-server": "^2.6.1"
  },
  "devDependencies": {
    "copyfiles": "^2.2.0",
    "imagemin-cli": "^5.1.0",
    "onchange": "^7.0.0",
    "parallelshell": "3.0.1",
    "rimraf": "^3.0.2",
    "usemin-cli": "^0.6.0"
  },
  "scripts": {
    "lite": "lite-server",
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "watch:all": "parallelshell 'npm run watch:scss' 'npm run lite'",
    "imagemin": "imagemin img/* -o dist/img",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html",
    "clean": "rimraf dist",
    "copyfonts": "copyfiles -f font/* dist/fonts",
    "build": "npm run clean && npm run imagemin && npm run copyfonts && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EstebanMPC/LCcampaign.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EstebanMPC/LCcampaign/issues"
  },
  "homepage": "https://github.com/EstebanMPC/LCcampaign#readme"
}
